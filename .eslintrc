{
    "extends": "eslint:recommended",
    "env": {
        "es6": true,
        "node": true,
        "browser": true
    },
    "parser": "babel-eslint",
    "rules": {
        "semi": ["error", "always"],
        "indent": ["error", 4, { "SwitchCase" : 1}],
        "quotes": [2, "single"],
        "strict": [2, "never"],
        "no-trailing-spaces": 0
    }
}
